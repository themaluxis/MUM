{
  "plugin_id": "my_service",
  "name": "My Media Service",
  "description": "Integration with My Media Service for user management and library access",
  "version": "1.0.0",
  "author": "Your Name",
  "license": "MIT",
  "homepage": "https://github.com/yourname/mum-my-service",
  "repository": "https://github.com/yourname/mum-my-service.git",
  
  "module_path": "my_service",
  "service_class": "MyServiceMediaService",
  
  "supported_features": [
    "user_management",
    "library_access",
    "active_sessions",
    "downloads"
  ],
  
  "min_mum_version": "1.0.0",
  "max_mum_version": null,
  
  "python_requirements": [
    "requests>=2.25.0"
  ],
  
  "config_schema": {
    "type": "object",
    "properties": {
      "timeout": {
        "type": "integer",
        "default": 10,
        "minimum": 5,
        "maximum": 60,
        "description": "Request timeout in seconds"
      },
      "verify_ssl": {
        "type": "boolean",
        "default": true,
        "description": "Verify SSL certificates"
      },
      "custom_setting": {
        "type": "string",
        "default": "",
        "description": "Custom setting specific to your service"
      }
    }
  },
  
  "default_config": {
    "timeout": 10,
    "verify_ssl": true,
    "custom_setting": ""
  }
}